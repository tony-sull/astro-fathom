{
  "name": "astro-fathom",
  "version": "1.0.0",
  "description": "Fathom Analytics tracking for your Astro site.",
  "type": "module",
  "exports": {
    ".": "./index.ts"
  },
  "files": [
    "index.ts",
    "Fathom.astro"
  ],
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tony-sull/astro-fathom.git"
  },
  "author": {
    "name": "Tony Sullivan",
    "email": "tony@astro.build",
    "url": "https://tonysull.co"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tony-sull/astro-fathom/issues"
  },
  "scripts": {
    "postinstall": "husky install",
    "lint": "run-p --print-label --continue-on-error lint:*",
    "lint:eslint": "eslint --ignore-path .eslintignore --ext js,jsx,ts,tsx,mjs,cjs,astro .",
    "lint:ts": "tsc --noEmit",
    "lint:astro": "astro check",
    "format": "run-s format:*",
    "format:prettier": "prettier -w --plugin-search-dir=. .",
    "format:eslint": "eslint --ignore-path .eslintignore --ext js,jsx,ts,tsx,mjs,cjs,astro --fix .",
    "format:imports": "organize-imports-cli ./tsconfig.json"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "astro": "^1.6.11",
    "astro-eslint-parser": "^0.9.4",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.8.2",
    "prettier-plugin-astro": "^0.7.2",
    "semantic-release": "^20.0.2"
  },
  "peerDependencies": {
    "astro": "^1.6.11"
  }
}
